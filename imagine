mouse.KeyDown:connect(function(key)
	if (VALUE1 == false and attack == false) or VALUE2 == true then
		if key == "q" then
			if VALUE2 == false then
				I_REALLY_CANT_STOP()
			elseif VALUE2 == true then
				VALUE2 = false
			end
		end
	end
	if attack == false then
		if key == 't' then
			FuckingDyingInside()
		elseif key == 'z' then
			SmackaCracka()
		elseif key == 'x' then
			OWOIMMASPINNING()
		elseif key == '123123123' then
			RAEP_qmq()
		elseif key == 'c' then
			local B = Cso("847665457", hed, 7, 1)
			B.TimePosition = 13.2
			CameraEnshaking(5, 5)
			local ORIGINPOS = root.Position
			root.CFrame = CFrame.new(Vector3.new(mouse.Hit.p.X, root.Position.Y, mouse.Hit.p.Z), ORIGINPOS)
			wait(1.3)
			B.Playing = false
		elseif key == 'v' then
			My_Favorite_Show_As_A_Kid()
		end
	end
end)
 






